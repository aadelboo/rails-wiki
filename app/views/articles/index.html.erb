
<div class="container">
  <div class="articles-group">
    <ul class= articles-ul>
      <% @articles.each do |article| %>
      <li> <%= link_to article.title, article_path(article), class:"article-title" %></li>
      <li class="article-content"> <%= article.content.first(200) %></li>
      <% end %>
    </ul>
  </div>
</div>
